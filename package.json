{
    "name": "@j-cake/mkjson",
    "description": "The make-like build tool that lives in your package.json.",
    "type": "module",
    "version": "v1.4.2",
    "bin": "run.js",
    "main": "build/lib.js",
    "keywords": [
        "json",
        "makefile",
        "make",
        "build",
        "package.json"
    ],
    "author": {
        "name": "J-Cake",
        "email": "jakieschneider13@gmail.com",
        "url": "https://jschneiderprojects.com.au"
    },
    "repository": "https://github.com/J-Cake/mkjson",
    "scripts": {
        "build:core": "node bin/build.js build:core",
        "build:cli": "node bin/build.js build:cli",
        "build:lib": "node bin/build.js build:lib",
        "build:plugin/json": "node bin/build.js build:plugin/json",
        "build:plugin/js": "node bin/build.js build:plugin/js",
        "build:plugin/glob": "node bin/build.js build:plugin/glob",
        "build:plugin/fs": "node bin/build.js build:plugin/fs",
        "build:plugin/fs_lsdir": "node bin/build.js build:plugin/fs_lsdir",
        "build:plugin/http": "node bin/build.js build:plugin/http",
        "build:meta": "node bin/build.js build:meta",
        "build": "node bin/build.js build",
        "clean": "node bin/build.js clean",
        "test": "node bin/build.js test"
    },
    "plugins": [
        "fs.js",
        "http.js",
        "json.js",
        "js.js",
        "glob.js"
    ],
    "dependencies": {
        "@j-cake/jcake-utils": "latest",
        "@j-cake/mkjson": "latest",
        "chalk": "latest",
        "lodash": "latest",
        "lsdir": "link:src/plugins/lsdir/build"
    },
    "devDependencies": {
        "@types/lodash": "latest",
        "@types/node": "latest",
        "esbuild": "latest",
        "typescript": "latest"
    },
    "optionalDependencies": {
        "json5": "latest"
    },
    "imports": {
        "lsdir": "src/plugins/lsdir",
        "#core": "./build/index.js",
        "#cli": "./build/cli.js"
    },
    "types": "build/lib.d.ts",
    "deploy": {
        "targets": null,
        "deploy": null,
        "scripts": null,
        "devDependencies": null,
        "optionalDependencie": null,
        "main": "./lib.js",
        "bin": "./run.js",
        "types": "./index.d.ts",
        "imports": {
            "#core": "./index.js",
            "#cli": "./cli.js"
        }
    }
}
